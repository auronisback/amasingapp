{
  "name": "amasingapp",
  "version": "1.0.1",
  "private": true,
  "homepage": "https://auronisback.github.io/amasingapp/",
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!<rootDir>/node_modules/"
    ],
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ]
  },
  "jestSonar": {
    "reportPath": "testResults",
    "reportFile": "sonar-report.xml",
    "indent": 4
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.15",
    "@fortawesome/free-solid-svg-icons": "^5.7.2",
    "@fortawesome/react-fontawesome": "^0.1.4",
    "@google/maps": "^0.5.5",
    "@types/googlemaps": "^3.30.16",
    "@types/markerclustererplus": "^2.1.33",
    "@types/react": "^16.8.6",
    "axios": "^0.18.0",
    "bootstrap": "^4.3.1",
    "gh-pages": "^2.0.1",
    "i": "^0.3.6",
    "jquery": "^3.3.1",
    "node-sass": "^4.11.0",
    "npm": "^6.8.0",
    "popper.js": "^1.14.7",
    "rc-slider": "^8.6.6",
    "react": "^16.8.3",
    "react-dom": "^16.8.3",
    "react-geocode": "^0.1.2",
    "react-google-maps": "^9.4.5",
    "react-scripts": "^2.1.8",
    "typescript": "^3.3.3333"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom --testResultsProcessor '<rootDir>'/node_modules/jest-sonar-reporter/index.js",
    "predeploy": "react-scripts build",
    "deploy": "gh-pages -d build",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "devDependencies": {
    "axios-mock-adapter": "^1.16.0",
    "enzyme": "^3.9.0",
    "enzyme-adapter-react-16": "^1.10.0",
    "enzyme-to-json": "^3.3.5",
    "jest-sonar-reporter": "^2.0.0",
    "react-test-renderer": "^16.8.4"
  }
}
